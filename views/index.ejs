                </div>
                <div class="category-section-list">
                    <ul>
                        <li><a href="javascript:void(0);" class="category-section-item selected">所有</a></li>
                        <% for (var category in config) { %>
                        <% var category_name = config[category].aka; %>
                            <li><a href="javascript:void(0);" class="category-section-item" title="<%= category_name %>"><%= category_name %></a></li>
                        <% } %>
                    </ul>
                </div>
            </div>
